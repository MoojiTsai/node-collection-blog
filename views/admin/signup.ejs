<%- include('partials/header.ejs') %>





  <div class="container panel-md">
    <div class="row">
      <div class="container">
        <h2>註冊</h2>
      </div>
    </div>
    <form  action="<%= form_action%>" method="post">
      <div class="form-group">
        <label>帳號</label>
        <input placeholder="帳號" class="form-control" type="text" name="account">
      </div>
      <div class="form-group">
        <label>密碼</label>
        <input placeholder="密碼" class="form-control" type="password" name="password">
      </div>
      <div class="form-group">
        <label>確認密碼</label>
        <input  class="form-control" placeholder="確認密碼" type="password" name="repassword">
      </div>

      <input type="submit" class="btn btn-primary" value="註冊">
    </form>

  </div>

